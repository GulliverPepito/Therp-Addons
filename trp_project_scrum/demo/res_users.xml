<odoo>

    <record id="collaborator_follower" model="res.users">
        <field name="name">collaborator_follower</field>
        <field name="groups_id"
            eval="[(4, ref('trp_project_scrum.collaborators_followers'), 0)]"/>
        <field name="login">login</field>
        <field name="password">password</field>
        <field name="context">{'no_reset_password': True}</field>
    </record>

</odoo>
