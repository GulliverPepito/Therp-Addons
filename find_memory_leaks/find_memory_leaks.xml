            <field name="model">find.memory.leaks</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
                <form col="6">
                    <field name="output" colspan="6" nolabel="1" />
                    <button type="object" name="button_typestats"
                        string="typestats" />
                    <button type="object" name="button_get_leaking_objects"
                        string="get_leaking_objects" />
